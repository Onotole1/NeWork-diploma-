<?xml version="1.0" encoding="utf-8"?>
  <androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
      xmlns:app="http://schemas.android.com/apk/res-auto"
      xmlns:tools="http://schemas.android.com/tools"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      tools:context=".ui.EditJobFragment">

      <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

          <TableRow
              android:id="@+id/tableRowStartDate"
              android:layout_width="match_parent"
              android:layout_height="@dimen/commonSpacingXLarge"
              android:layout_margin="@dimen/marginEnd"
              android:orientation="vertical"
              app:layout_constraintTop_toBottomOf="parent">

            <TextView
                android:id="@+id/startDate"
                android:layout_width="@dimen/commonSpacingXXSLarge"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:text="@string/startDate" />

            <EditText
                android:id="@+id/startDateEdit"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:cursorVisible="false"
                android:focusable="false"
                android:importantForAutofill="no"
                android:longClickable="false"
                android:textSize="@dimen/editTextSize"
                app:layout_constraintTop_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="LabelFor,TextFields"
                />
          </TableRow>

          <TableRow
              android:id="@+id/tableRowFinishDate"
              android:layout_width="match_parent"
              android:layout_height="@dimen/commonSpacingXLarge"
              android:layout_margin="@dimen/marginEnd"
              android:orientation="vertical"
              app:layout_constraintTop_toBottomOf="parent">

            <TextView
                android:id="@+id/finishDate"
                android:layout_width="@dimen/commonSpacingXXSLarge"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:text="@string/finishDate" />

            <EditText
                android:id="@+id/finishDateEdit"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:cursorVisible="false"
                android:focusable="false"
                android:importantForAutofill="no"
                android:longClickable="false"
                android:textSize="@dimen/editTextSize"
                app:layout_constraintTop_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="LabelFor,TextFields"
                />
          </TableRow>
          <TableRow
              android:id="@+id/tableRowPosition"
              android:layout_width="match_parent"
              android:layout_height="@dimen/commonSpacingXLarge"
              android:layout_margin="@dimen/marginEnd"
              android:orientation="vertical"
              app:layout_constraintTop_toBottomOf="parent">

            <TextView
                android:id="@+id/position"
                android:gravity="center_vertical"
                android:layout_width="@dimen/commonSpacingXXSLarge"
                android:layout_height="@dimen/commonSpacingSLarge"
                android:text="@string/position" />

            <EditText
                android:id="@+id/positionEdit"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:cursorVisible="false"
                android:focusable="false"
                android:importantForAutofill="no"
                android:longClickable="false"
                app:layout_constraintTop_toBottomOf="parent"
                tools:ignore="LabelFor,TextFields" />
          </TableRow>
          <TableRow
              android:id="@+id/tableRowCompany"
              android:layout_width="match_parent"
              android:layout_height="@dimen/commonSpacingXLarge"
              android:layout_margin="@dimen/marginEnd"
              android:orientation="vertical"
              app:layout_constraintTop_toBottomOf="parent">

            <TextView
                android:id="@+id/company"
                android:gravity="center_vertical"
                android:layout_width="@dimen/commonSpacingXXSLarge"
                android:layout_height="@dimen/commonSpacingSLarge"
                android:text="@string/nameCompany" />

            <EditText
                android:id="@+id/companyEdit"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:cursorVisible="false"
                android:focusable="false"
                android:importantForAutofill="no"
                android:longClickable="false"
                app:layout_constraintTop_toBottomOf="parent"
                tools:ignore="LabelFor,TextFields" />
          </TableRow>
          <TableRow
              android:id="@+id/tableRowLink"
              android:layout_width="match_parent"
              android:layout_height="@dimen/commonSpacingXLarge"
              android:layout_margin="@dimen/marginEnd"
              android:orientation="vertical"
              app:layout_constraintTop_toBottomOf="parent">

            <TextView
                android:id="@+id/link"
                android:layout_width="@dimen/commonSpacingXSLarge"
                android:layout_height="@dimen/commonSpacingXLarge"
                android:gravity="center_vertical"
                android:text="@string/link_event" />

            <EditText
                android:id="@+id/link_edit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:importantForAutofill="no"
                android:inputType="textUri"
                app:layout_constraintTop_toBottomOf="parent"
                tools:ignore="LabelFor" />
          </TableRow>

        </LinearLayout>


    </ScrollView>

</androidx.coordinatorlayout.widget.CoordinatorLayout>